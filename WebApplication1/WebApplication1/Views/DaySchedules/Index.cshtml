@model IEnumerable<WebApplication1.Models.DaySchedule>

@{
    ViewData["Title"] = "Programari";
}

<h2>Programari</h2>

<p>
    <a asp-action="Create"> New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdDayNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdDoctorNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdHourNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdDayNavigation.IdDay)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdDoctorNavigation.IdDoctor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdHourNavigation.IdHour)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdSchedule">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdSchedule">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdSchedule">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
