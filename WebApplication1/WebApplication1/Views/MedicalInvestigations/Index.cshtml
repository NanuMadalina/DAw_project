@model IEnumerable<WebApplication1.Models.MedicalInvestigations>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.InvName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InvDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InvPret)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.InvName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InvDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InvPret)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdInvestigation">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdInvestigation">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdInvestigation">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
